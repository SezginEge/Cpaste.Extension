{
  "name": "Cpaste",
  "description": "Simplifies copy paste between different devices",
  "version": "0.0.1",
  "browser_action": {
      "default_title": "Cpaste: Online Clipboard",
      "default_icon": "icons/icon16.png",
      "default_popup": "index.html"
  },
  "manifest_version": 2,
  "permissions": ["http://cpaste.me/*"],
  "icons": { "16": "icons/icon16.png",
           "48": "icons/icon48.png",
          "128": "icons/icon128.png" }
}
